{
  "name": "@process-engine/process_engine_meta_integration_tests",
  "publishConfig": {
    "registry": "https://www.npmjs.com"
  },
  "version": "0.0.1",
  "description": "integration tests",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "tests"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=sqlite CONFIG_PATH=config node index.js",
    "test-sqlite": "cross-env NODE_ENV=sqlite CONFIG_PATH=config mocha -t 15000 test/*.js --exit",
    "test-postgres": "cross-env NODE_ENV=postgres CONFIG_PATH=config mocha -t 15000 test/*.js --exit",
    "test-mysql": "cross-env NODE_ENV=mysql CONFIG_PATH=config mocha -t 15000 test/*.js --exit",
    "test": "npm run test-sqlite && npm run test-postgres",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build-commonjs && npm run build-amd",
    "build-commonjs": "tsc",
    "build-amd": "tsc --module amd --outDir ./dist/amd",
    "lint": "eslint src/**/*.ts src/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts src/*.ts"
  },
  "dependencies": {
    "@essential-projects/bootstrapper": "~3.4.0",
    "@essential-projects/bootstrapper_node": "~3.3.0",
    "@essential-projects/event_aggregator": "~4.1.0",
    "@essential-projects/http": "~2.5.0",
    "@essential-projects/http_extension": "~7.1.0",
    "@essential-projects/timing": "~5.0.0",
    "@process-engine/consumer_api_client": "~6.4.0",
    "@process-engine/consumer_api_core": "6.5.0-beta.1",
    "@process-engine/consumer_api_http": "~5.5.1",
    "@process-engine/iam": "~1.7.1",
    "@process-engine/logging_api_core": "~2.0.2",
    "@process-engine/logging.repository.file_system": "~2.0.2",
    "@process-engine/process_engine_core": "12.17.1-beta.1",
    "@process-engine/persistence_api.repositories.sequelize": "~1.4.2",
    "@process-engine/persistence_api.services": "~1.4.0",
    "@process-engine/persistence_api.use_cases": "~1.4.0",
    "addict-ioc": "^2.5.6",
    "bluebird": "^3.7.2",
    "jsonwebtoken": "^8.5.0",
    "loggerhythm": "^3.0.3",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "node-uuid": "^1.4.8",
    "pg": "^7.18.0",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.1",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "@essential-projects/eslint-config": "^1.2.0",
    "@essential-projects/iam_contracts": "^3.6.0",
    "@types/bluebird": "^3.5.26",
    "@types/express": "^4.17.0",
    "@types/node": "^12.11.7",
    "@types/socket.io": "^2.1.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/validator": "^12.0.0",
    "cross-env": "^7.0.0",
    "mocha": "^7.0.0",
    "should": "^13.2.3",
    "typescript": "^3.7.5"
  }
}
